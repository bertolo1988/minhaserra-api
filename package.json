{
  "name": "minhaserra-api",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "format": "npx prettier '*.{js,ts,json}' --write",
    "lint": "eslint . --ext .ts",
    "test:format": "npx prettier '*.{js,ts,json}' --check",
    "start": "node dist/src/run.js",
    "start:dev": "npx ts-node src/run.ts",
    "test:unit": "cross-env NODE_ENV=test jest test/unit",
    "test:integration": "cross-env NODE_ENV=test jest test/integration/all-integration-tests.spec.ts",
    "seed:create": "npx knex seed:make empty_seed --knexfile knexfile.ts",
    "seed:run": "npx knex seed:run --knexfile knexfile.ts",
    "seed:clean": "npx knex seed:run --knexfile knexfile.ts --specific=clean-database.seed.ts",
    "migrate:latest": "npx knex migrate:latest --knexfile knexfile.ts",
    "migrate:rollback": "npx knex migrate:rollback --all --knexfile knexfile.ts",
    "migrate:up": "npx knex migrate:up --knexfile knexfile.ts",
    "migrate:down": "npx knex migrate:down --knexfile knexfile.ts",
    "migrate:list": "npx knex migrate:list --knexfile knexfile.ts",
    "migrate:new": "npx knex migrate:make my-new-migration -x ts --knexfile knexfile.ts"
  },
  "keywords": [],
  "author": "bertolo1988 <tiagobertolo@gmail.com> (https://github.com/bertolo1988)",
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "aws-sdk": "^2.1574.0",
    "change-case-all": "^2.1.0",
    "dotenv": "^16.4.5",
    "eslint-plugin-import": "^2.29.1",
    "http-graceful-shutdown": "^3.1.13",
    "iso-3166-1-alpha-2": "^1.0.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "moment": "^2.30.1",
    "pg": "^8.11.3",
    "pg-error-codes-ts": "^1.0.1",
    "sharp": "^0.33.3",
    "uuid": "^9.0.1",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@types/change-case": "^2.3.1",
    "@types/iso-3166-1-alpha-2": "^1.0.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/knex": "^0.16.1",
    "@types/koa": "^2.15.0",
    "@types/koa-logger": "^3.1.5",
    "@types/koa-router": "^7.4.8",
    "@types/lodash": "^4.14.202",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.11.25",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "timekeeper": "^2.3.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
